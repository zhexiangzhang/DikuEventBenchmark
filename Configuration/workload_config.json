{
  "transactionDistribution": {
    "CUSTOMER_SESSION": 70, 
    "DASHBOARD": 75,
    "PRICE_UPDATE": 82, // 82
    "DELETE_PRODUCT": 85,
    "UPDATE_DELIVERY": 100
  },
  // not inside customer config because customer
  // worker does not need to know that
  "customerDistribution": "NON_UNIFORM",
  "executionTime": 60000, // 60000 = 1 minute by default 360000
  "delayBetweenRequests": 0, 
  "concurrencyLevel": 6, // 10000, 64
  "endToEndLatencyCollection": false,
  "streamingConfig": {
    "type": "REDIS",
    "host": "localhost",
    "port": 6379,
    "streams": [
      "ReserveStock",
      "Product",
      "InvoiceIssued",
      "PaymentConfirmed",
      "PaymentFailed",
      "ShipmentNotification",
      "DeliveryNotification",
      "StockItem",
      "StockConfirmed",
      "ReserveStockFailed"
    ]
  },
  "customerWorkerConfig": {
    "maxNumberKeysToBrowse": 10,
    "maxNumberKeysToAddToCart": 10,
    "sellerDistribution": "NON_UNIFORM",
    "minMaxQtyRange": {
      "min": 1,
      "max": 10
    },
    "delayBetweenRequestsRange": {
      "min": 1,
      "max": 1000
    },
    "checkoutProbability": 100,
    "voucherProbability": 5,
    "maxNumberVouchers": 10,
    "productUrl": "http://127.0.0.1:8090",
    "cartUrl": "http://127.0.0.1:8090",
    "browsing": true,
    "targetPlatform": "STATEFUN"    
  },
  "sellerWorkerConfig": {
    "keyDistribution": "NON_UNIFORM",
    "delayBetweenRequestsRange": {
      "min": 1,
      "max": 1000
    },
    "sellerUrl": "http://127.0.0.1:8090",
    "productUrl": "http://127.0.0.1:8090",
    "targetPlatform": "STATEFUN",
    "productPartion": 10 // only for statefun
  },
  "deliveryWorkerConfig": {
    "shipmentUrl": "http://127.0.0.1:8090",
    "targetPlatform": "STATEFUN",
    "proxyAddress": 0 // only for statefun
  }
}

