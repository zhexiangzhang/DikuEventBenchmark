{
  "enabled": true,
  "connectionString": "DataSource=:memory:",
  "numCustomers": 10000,
  "numProdPerSeller": 10,
  "executionTime": 60000,
  "epoch": 2000,
  "delayBetweenRequests": 0,
  "delayBetweenRuns": 15000,
  "transactionDistribution": {
    "CUSTOMER_SESSION": 30,
    "QUERY_DASHBOARD": 35,
    "PRICE_UPDATE": 38,
    "UPDATE_PRODUCT": 40,
    "UPDATE_DELIVERY": 100
  },
  "concurrencyLevel": 512,
  "ingestionConfig": {
      "strategy": "WORKER_PER_CPU",
      "concurrencyLevel": 64,
      "mapTableToUrl": {
          "sellers": "http://dapr:5006",
          "customers": "http://dapr:5007",
          "stock_items": "http://dapr:5003",
          "products": "http://dapr:5008"
      },
      "kafkaService" : "kafkahost:9092",    
      "kafkaStreamProvider": "SMSProvider",
      "kafkaIngestTopics": {
          "initCustomer": "customer",
          "initSeller": "seller",
          "addStockItems": "stock",
          "addProducts": "product"
      },
      //only for stateFun
      "productPartion": 10000000, 
      "sellerPartion": 10000000, // must 1:1 !!
      "customerPartion": 10000000, // must 1:1 !!
      "stockPartion": 10000000
  },
  "stateFunUrl": "http://statefunhost:8081/",
  "runs": [
    {
      "numProducts": 10000,
      "customerDistribution": "UNIFORM",
      "sellerDistribution": "UNIFORM",
      "keyDistribution": "UNIFORM"
    }
    // ,
    // {
    //   "numProducts": 1000,
    //   "customerDistribution": "UNIFORM",
