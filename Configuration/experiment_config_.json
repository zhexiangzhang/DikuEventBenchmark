    //   "numProducts": 10000,
    //   "customerDistribution": "UNIFORM",
    //   "sellerDistribution": "ZIPFIAN",
    //   "keyDistribution": "ZIPFIAN"
    // }
  ],
  "postRunTasks": [
    {
      "name": "cart",
      "url": "http://dapr:5001/reset"
    },
    {
      "name": "order",
      "url": "http://dapr:5002/cleanup"
    },
    {
      "name": "stock",
      "url": "http://dapr:5003/reset"
    },
    {
      "name": "payment",
      "url": "http://dapr:5004/cleanup"
    },
    {
      "name": "shipment",
      "url": "http://dapr:5005/cleanup"
    },
    {
      "name": "seller",
      "url": "http://dapr:5006/reset"
    },
    {
      "name": "customer",
      "url": "http://dapr:5007/reset"
    },
    {
      "name": "product",
      "url": "http://dapr:5008/reset"
    }
  ],
  "postExperimentTasks": [
    {
      "name": "cart",
      "url": "http://dapr:5001/cleanup"
    },
    {
      "name": "order",
      "url": "http://dapr:5002/cleanup"
    },
    {
      "name": "stock",
      "url": "http://dapr:5003/cleanup"
    },
    {
      "name": "product",
      "url": "http://dapr:5008/cleanup"
    },
    {
      "name": "payment",
      "url": "http://dapr:5004/cleanup"
    },
    {
      "name": "shipment",
      "url": "http://dapr:5005/cleanup"
    },
    {
      "name": "seller",
      "url": "http://dapr:5006/cleanup"
    },
    {
      "name": "customer",
      "url": "http://dapr:5007/cleanup"
    },
    {
      "name": "product",
      "url": "http://dapr:5008/cleanup"
    }
  ],
  "streamingConfig": {
    "type": "REDIS",
    "host": "redis",
    "port": 6379,
    "streams": [
      "DeliveryNotification",
      "InvoiceIssued",
      "PaymentConfirmed",
      "PaymentFailed",
      "Product",
      "ProductUpdate",
      "ReserveStock",
      "ShipmentNotification",
      "StockConfirmed",
      "ReserveStockFailed"
    ]
  },    
  "kafkaService" : "kafkahost:9092",    
  "kafkaConsumerTopics": [
    "checkoutTask",
    "updateProductTask",
    "updatePriceTask",
    "updateDeliveryTask",
    "queryDashboardTask"
  ],
  "kafkaProducerTopics":[
    "priceUpdate", 
    "productUpdate",
    "queryDashboard",              
    "customerSession",
    "updateDelivery" 
  ],
  "customerWorkerConfig": {
    "maxNumberKeysToBrowse": 10,
    "maxNumberKeysToAddToCart": 10,
    "minMaxQtyRange": {
      "min": 1,
      "max": 10
    },
    "delayBetweenRequestsRange": {
      "min": 1,
      "max": 1000
    },
    "checkoutProbability": 100,
    "voucherProbability": 5,
    "maxNumberVouchers": 10,
    "productUrl": "http://dapr:5008",
    "cartUrl": "http://dapr:5001",
    "interactive": false
  },
  "sellerWorkerConfig": {
    "interactive": false,
    "delayBetweenRequestsRange": {
      "min": 1,
      "max": 1000
    },
    "adjustRange": {
      "min": 1,
      "max": 10
    },
    "sellerUrl": "http://dapr:5006",
    "productUrl": "http://dapr:5008",
    "productPartion": 10000000
  },
  "deliveryWorkerConfig": {
    "shipmentUrl": "http://dapr:5005"
  }
